  "scripts": {
    "clean": "rm -r build",
    "start": "NODE_ENV=development webpack serve --open",
    "build": "yarn clean && cp -r public build && BUILD_MODE=development webpack",
    "build:production": "yarn lint && yarn clean && cp -r public build && BUILD_MODE=production webpack",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "deploy": "ssh omame.xyz rm -rf /var/www/mainsite/assets && yarn build:production && rsync -a --progress --usermap omame:http build/. omame.xyz:/var/www/mainsite/"
  },
